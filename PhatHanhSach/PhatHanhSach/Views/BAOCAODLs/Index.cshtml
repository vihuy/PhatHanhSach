@model IEnumerable<PhatHanhSach.Models.BAOCAODL>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Mã Đại lý")
        </th>
        <th>
            @Html.DisplayName("Tên Đại lý")
        </th>
        <th>
            @Html.DisplayName("Thanh toán")
        </th>
        <th>
            @Html.DisplayName("Thời gian")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaDL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DAILY.Ten)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThanhToan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThoiGian)
        </td>
        <td>
            @Html.ActionLink("Chi tiết", "Details", new { id=item.MaBCDL })
        </td>
    </tr>
}

</table>
